{
  "name": "manhattanman.backend",
  "version": "0.0.1",
  "description": "Backend for the personal blog",
  "main": "dist/index.js",
  "scripts": {
    "start": "DEBUG=@neo4j/graphql:* yarn debug",
    "watch": "concurrently \"yarn watch:ts\" \"yarn nodemon\"",
    "watch:ts": "tsc -w",
    "debug": "concurrently \"yarn watch:ts\" \"yarn nodemon\"",
    "nodemon:dist": "nodemon dist/server.js",
    "nodemon": "nodemon",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.{js,ts}\" --write --loglevel warn",
    "fix:eslint": "eslint 'src/**/*.{js,ts}' --quiet --fix",
    "fix:types": "tsc -p . --noEmit",
    "generate:types": "graphql-codegen --config .graphqlrc.yaml --project generator",
    "test": "jest --runInBand",
    "test:watch": "yarn test --watch",
    "test:debug": "node --inspect-brk=localhost:9236  --preserve-symlinks -r ts-node/register ./node_modules/.bin/jest --runInBand"
  },
  "keywords": [
    "neo4j",
    "blog",
    "backend",
    "graphql"
  ],
  "author": "Igor Kulebyakin",
  "license": "ISC",
  "dependencies": {
    "@neo4j/graphql": "^3.0.1",
    "@neo4j/graphql-ogm": "^3.0.1",
    "@neo4j/graphql-plugin-auth": "^1.0.0",
    "neo4j-driver": "^4.4.3",
    "apollo-server": "^3.6.3",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.1",
    "graphql": "^16.3.0",
    "generate-password": "^1.7.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "passport-local-authenticate": "^1.2.0",
    "passport-vkontakte": "^0.5.0",
    "semver": "^7.3.5",
    "@graphql-tools/load-files": "^6.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-resolvers": "^2.5.2",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/passport-vkontakte": "^1.3.1",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "tsconfig-paths": "^3.12.0",
    "babel-jest": "^27.5.1",
    "concurrently": "^7.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "~8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}